{
  "name": "command-central",
  "displayName": "Command Central",
  "description": "Code changes, sorted by time. See what changed, in the order it changed.",
  "version": "0.3.2",
  "publisher": "oste",
  "icon": "resources/icons/icon.png",
  "type": "module",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.100.0",
    "node": ">=18.0.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "keywords": [
    "ai",
    "command",
    "git",
    "sort",
    "scm",
    "productivity",
    "git sort",
    "sorted changes",
    "time sort",
    "workspace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ostehost/command-central"
  },
  "bugs": {
    "url": "https://github.com/ostehost/command-central/issues"
  },
  "homepage": "https://partnerai.dev",
  "galleryBanner": {
    "color": "#161b22",
    "theme": "dark"
  },
  "license": "MIT",
  "author": {
    "name": "Mike",
    "url": "https://github.com/ostehost"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true,
      "description": "Command Central works in all workspaces",
      "restrictedConfigurations": [
        "commandCentral.terminal.launcherPath",
        "commandCentral.terminal.app"
      ]
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "commandCentral",
          "title": "Command Central",
          "icon": "resources/icons/activity-bar.svg"
        }
      ],
      "panel": [
        {
          "id": "commandCentralPanel",
          "title": "Command Central",
          "icon": "resources/icons/activity-bar.svg"
        }
      ]
    },
    "configuration": {
      "title": "Command Central",
      "properties": {
        "commandCentral.project.icon": {
          "type": "string",
          "markdownDescription": "Icon/emoji to display in status bar for this workspace (e.g., `ðŸš€`, `ðŸ“¦`, `ðŸŽ¨`)",
          "examples": [
            "ðŸš€",
            "ðŸ“¦",
            "ðŸŽ¨",
            "ðŸ”§",
            "ðŸ“š",
            "ðŸ§ª",
            "ðŸŽ®",
            "ðŸŒŸ"
          ],
          "scope": "resource"
        },
        "commandCentral.project.name": {
          "type": "string",
          "description": "Display name for this workspace project",
          "scope": "resource"
        },
        "commandCentral.gitSort.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable sorting of git changes by modification time",
          "scope": "window"
        },
        "commandCentral.gitSort.fileTypeFilter": {
          "type": "string",
          "enum": [
            "all",
            "code",
            "config",
            "docs",
            "images",
            "tests",
            "custom"
          ],
          "default": "all",
          "description": "Filter files by type in sorted changes view",
          "enumDescriptions": [
            "Show all file types",
            "Show code files (.ts, .js, .py, .java, etc.)",
            "Show config files (.json, .yaml, .toml, etc.)",
            "Show documentation files (.md, .txt, etc.)",
            "Show image files (.png, .jpg, .svg, etc.)",
            "Show test files (.test.ts, .spec.js, etc.)",
            "Show custom file types defined in customFileTypes setting"
          ],
          "scope": "window"
        },
        "commandCentral.gitSort.customFileTypes": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^\\.[a-zA-Z0-9]+$"
          },
          "default": [],
          "description": "Custom file extensions to filter when fileTypeFilter is set to 'custom' (e.g., ['.vue', '.svelte'])",
          "scope": "window"
        },
        "commandCentral.trackActiveFile": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Automatically reveal and highlight the active file in the git changes tree view (matches VS Code Explorer's behavior)",
          "scope": "window"
        },
        "commandCentral.fileFilter.persistence": {
          "type": "string",
          "enum": [
            "workspace",
            "global",
            "none"
          ],
          "default": "workspace",
          "description": "Where to save extension filter preferences",
          "enumDescriptions": [
            "Save filters per workspace (recommended - filters are workspace-specific)",
            "Save filters globally (same filters across all workspaces)",
            "Don't save filters (reset on extension reload)"
          ],
          "scope": "window"
        },
        "commandCentral.fileFilter.maxExtensions": {
          "type": "number",
          "default": 50,
          "minimum": 10,
          "maximum": 200,
          "description": "Maximum number of extensions to show in picker (performance limit)",
          "scope": "window"
        },
        "commandCentral.statusBar.showProjectIcon": {
          "type": "boolean",
          "default": true,
          "description": "Show project icon in status bar",
          "scope": "window"
        },
        "commandCentral.statusBar.priority": {
          "type": "number",
          "default": 10000,
          "description": "Priority for status bar positioning (higher = further left)",
          "scope": "window"
        },
        "commandCentral.gitStatusGrouping.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable grouping of Git status changes by status type (staged/unstaged) with time-based subgroups",
          "scope": "application"
        },
        "commandCentral.agentTasksFile": {
          "type": "string",
          "default": "~/projects/ghostty-launcher/scripts/tasks.json",
          "markdownDescription": "Path to the agent task registry JSON file (tasks.json). Supports `~` expansion.",
          "scope": "machine"
        },
        "commandCentral.ghostty.launcherPath": {
          "type": "string",
          "default": "",
          "markdownDescription": "Path to the `launcher` binary (ghostty-launcher). Leave empty to use `launcher` from PATH, or the default fallback at `~/projects/ghostty-launcher/launcher`.",
          "scope": "machine"
        }
      }
    },
